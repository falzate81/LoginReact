{
  "name": "SerPlus",
  "version": "1.0.2",
  "private": true,
  "src": "./",
  "dist": "./builds/web",
  "main": "electron.js",
  "scripts": {
    "mobile:start": "adb reverse tcp:8081 tcp:8081 && react-native start",
    "android:bundle": "react-native bundle --entry-file src/index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ --platform android --dev false",
    "android:run": "react-native run-android",
    "android:shake": "adb shell input keyevent 82",
    "android:log": "react-native log-android",
    "web-desktop:build": "grunt build",
    "web:serve": "grunt serve-web",
    "web:serve:dist": "grunt serve-web:dist",
    "desktop:serve": "grunt serve-electron",
    "desktop:package:linux:86": "electron-packager . SerPlusClient --arch=x86 --platform=linux --out=builds/desktop",
    "desktop:package:linux:64": "electron-packager . SerPlusClient --arch=x64 --platform=linux --out=builds/desktop",
    "desktop:package:windows:86": "electron-packager . SerPlusClient --arch=x86 --platform=win32 --out=builds/desktop",
    "desktop:package:windows:64": "electron-packager . SerPlusClient --arch=x64 --platform=win32 --out=builds/desktop",
    "desktop:package:all": "electron-packager . SerPlusClient --all --out=builds/desktop",
    "storybook": "start-storybook -s ./src/assets -p 6006",
    "build-storybook": "build-storybook",
    "api:db:regen": "grunt generate-mock",
    "api:mock:local": "json-server --port 3000 --watch apiMock/db.json --middlewares apiMock/middlewares/auth.js apiMock/middlewares/routes.js --routes apiMock/routes.json",
    "api:mock:lan": "json-server --port 3000 --watch apiMock/db.json --middlewares apiMock/middlewares/auth.js apiMock/middlewares/routes.js --routes apiMock/routes.json --host `cat apiMock/.host`",
    "lint": "grunt eslint"
  },
  "dependencies": {
    "axios": "^0.16.0",
    "history": "^4.7.2",
    "core-js": "^2.5.1",
    "joi": "^11.0.3",
    "joi-browser": "^10.6.1",
    "keymirror": "^0.1.1",
    "lowdb": "^0.16.0",
    "material-ui": "^0.17.3",
    "material-ui-rating": "^1.2.0",
    "object-assign": "^4.1.1",
    "react": "^15.4.2",
    "react-d3-basic": "^1.6.11",
    "react-dom": "^15.4.2",
    "react-donut-chart": "^1.0.5",
    "react-flexbox-grid": "^1.0.2",
    "react-inlinesvg": "^0.6.0",
    "react-native": "0.43.0",
    "react-native-fs": "^2.1.0-rc.1",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-form": "^4.1.3",
    "redux-saga": "^0.15.3"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "@kadira/storybook-addon-options": "^1.0.2",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-react-native": "1.9.1",
    "css-loader": "^0.28.1",
    "electron": "^1.6.2",
    "electron-packager": "^8.6.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-react-native": "^2.3.2",
    "extract-text-webpack-plugin": "^2.1.2",
    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "grunt-concurrent": "^2.3.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-connect": "^1.0.2",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^19.0.0",
    "grunt-exec": "^2.0.0",
    "grunt-open": "^0.2.3",
    "grunt-sass": "^2.0.0",
    "grunt-webpack": "^2.0.1",
    "json-server": "0.8.22",
    "load-grunt-tasks": "^3.5.2",
    "node-sass": "^4.5.2",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^5.0.3",
    "react-storybook-addon-backgrounds": "^0.0.7",
    "react-storybook-story": "^2.2.0",
    "redux-logger": "^3.0.1",
    "sass-loader": "^6.0.3",
    "storybook-readme": "^2.0.2",
    "storybook-router": "^0.2.6",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-server": "^2.4.5"
  }
}
